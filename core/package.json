{
  "name": "@ontrack/source",
  "version": "1.0.0",
  "license": "MIT",
  "workspaces": ["apps/**/*", "libs/**/*"],
  "engines": {
    "node": "20"
  },
  "scripts": {
    "start:all": "yarn nx run-many --target=serve --all",
    "serve-selected": "yarn nx run-many --target=serve --parallel --projects",
    "lint:all": "yarn nx run-many --target=lint --all",
    "test": "yarn nx affected -t test --base=staging --exclude=tag:excludeFromTest",
    "format:check": "yarn nx affected --target=format:check",
    "format": "prettier --write 'nx.json' 'tsconfig.base.json' '**/*.{ts,js,json,md}'",
    "typecheck": "yarn nx affected --target=build --configuration=type-check",
    "lint": "yarn nx affected --target=lint --base=staging && yarn lint:pkgjson",
    "lint:pkgjson": "npmPkgJsonLint .",
    "prepare": "husky || true",
    "reset:cache": "yarn nx reset"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "18.2.10",
